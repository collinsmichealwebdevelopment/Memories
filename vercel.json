{
    "rewrites": [
        { 
            "source": "/(.*)", 
            "destination": "/",
            "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
            "headers": {
                "Access-Control-Allow-Origin": "*"
            }
        }
    ],
    "builds": [
        {
            "src": "src/main.tsx",
            "use": "@vercel/node"
        }
    ]
}
