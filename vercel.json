{
    "rewrites": [
        { "source": "/(.*)", "destination": "/"}
    ],
    "builds": [
        {
            "src": "src/main.tsx",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/main.tsx",
            "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
            "headers": {
                "Access-Control-Allow-Origin": "*"
            }
        }
    ]
}
